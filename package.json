{
  "name": "security-badger",
  "version": "0.0.1",
  "description": "GitHub Action that posts to Slack with relevant GitHub security alerts",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "ncc build ./src/index.js",
    "start": "node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicklemmon/security-badger.git"
  },
  "author": "Nick Lemmon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicklemmon/security-badger/issues"
  },
  "homepage": "https://github.com/nicklemmon/security-badger#readme",
  "dependencies": {
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "prettier": "2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
